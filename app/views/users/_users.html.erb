<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
      <li class="media">
        <div class="media-body">
          <div>ユーザー名:<%= user.name %></div>
          <div>
            <% if current_user == user %>
            <p><%= link_to 'View profile', user_path(user) %></p>
            <% end %>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>